SELECT REGN_ABBR_NAME,DAY_NUM, MTH_NUM,CUST_ID,GEO_ID,PROD_ID,PROFT_CTR_ID,SITE_ID,BUOM_ID,PROD_CSU_TYPE_CODE,CATEG_NAME,ORG_CODE,CUST_GRP_NAME,GLDN_CUST_ID,GLDN_CUST_NAME,CNTRY_CLSTR_CODE,FAMLY_1_NAME,FAMLY_2_NAME,FAMLY_3_NAME,DP_ISO_CNTRY_CODE,MRKT_SGMNT_CODE,PLAN_AREA_NAME,SFU_VERS_NUM,SUB_REGN_NAME,STAT_UNIT_QTY,BASE_UNIT_QTY
FROM IDPDM.SD_OO_FCT
WHERE PROD_ID IN (SELECT distinct PROD_ID FROM IDPDM.MD_PROD_9005_DAY_FDIM
WHERE PROD_4_NAME IN ('AIR','CLEANPROD','CLEANSYS','AUTODISH','HANDDISH'))
AND DAY_NUM > 20220101
AND SUB_REGN_NAME = ('EU')
AND ORG_CODE = 'R'
