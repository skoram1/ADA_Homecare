SELECT TO_DATE(FRCST_VERS_DATE, 'yyyyMMdd') AS FRCST_VERS_DATE, TO_DATE(TP_END_DATE, 'yyyyMMdd') AS TP_END_DATE, PROD_ID, CNTRY_CLSTR_CODE, CUST_GRP_NAME, SITE_ID, FILE_SUB_REGN_NAME, SUB_REGN_NAME, ORG_CODE, BUOM_ID, DPEND_DEMND_FRCST_QTY, FINAL_FRCST_NORM_QTY, STAT_FRCST_QTY, MANUL_ADJMT_STAT_FRCST_QTY, BRAND_BLDG_FRCST_QTY, PRMTN_GO_TO_MKT_QTY, NEW_CHANL_FRCST_QTY, INITV_QTY, PRCNG_STRGY_QTY, MKT_TRND_QTY, CMPTV_ACTVY_QTY, CUST_INVEN_STRGY_QTY, BOP_ADJMT_QTY, FREE_GOODS_QTY, BAL_DETL_ASSUM_QTY, LOW_ADJMT_QTY, TOT_DETL_ASSUM_OVRAL_QTY, IGRTD_FRCST_UPLOD_QTY, MKT_INTLG_FRCST_1_QTY, MKT_INTLG_FRCST_2_QTY, FINAL_FRCST_FIN_QTY, SUPPL_FRCST_UPLOD_QTY, FINAL_FRCST_SUPPL_GDF_QTY, FINAL_FRCST_SUPPL_IDF_QTY, FINAL_FRCST_SUPPL_GDF_IDF_QTY, HIGH_ADJMT_QTY, SU_FACTR_RATE
FROM IDPDM.APO_FRCST_WK_FCT
WHERE FRCST_VERS_DATE >= 20220601
AND TP_END_DATE <= date_format(TO_DATE(FRCST_VERS_DATE, 'yyyyMMdd')+62, 'yyyyMMdd') 
AND SUB_REGN_NAME = ('EU')
AND ORG_CODE = 'R'
AND PROD_ID IN (SELECT distinct PROD_ID FROM IDPDM.MD_PROD_9005_DAY_FDIM
WHERE PROD_4_NAME IN ('AIR','CLEANPROD','CLEANSYS','AUTODISH','HANDDISH'))
